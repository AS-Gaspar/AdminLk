{
  "name": "adminlk",
  "version": "1.0.0",
  "description": "Basic admin aplication to a builder company",
  "license": "ISC",
  "author": "gaspar",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx webpack --config webpack.config.js",
    "dev": "webpack --config webpack.config.js --watch",
    "dev:serve": "nodemon --watch dist --exec \"node app.js\"",
    "dev:tailwind": "npx tailwindcss -i ./src/styles/input.css -o ./public/main.css --watch",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:serve\" \"npm run dev:tailwind\""
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.7",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "pg": "^8.16.0",
    "postgres": "^3.4.7",
    "tailwindcss": "^4.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.27.3",
    "@babel/preset-env": "^7.27.2",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
